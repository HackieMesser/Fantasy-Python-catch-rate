players = [{
   'name': 'Cooper Kupp',
   'catches': 145,
   'targets': 191
   },
   {
   'name': 'Deebo Samuel',
   'catches': 77,
   'targets': 121
   },
   {
   'name': 'Justin Jefferson',
   'catches': 108,
   'targets': 167
   },
]


for player in players:
    name = player["name"]
    catches = player["catches"]
    targets = player["targets"]
    catch_rate = catches/targets
    print(name + ' has a catch rate of ' + str(catch_rate))
